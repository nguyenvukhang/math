  "version": "0.0.1",
  "scripts": {
    "checkhealth": "node ./scripts/checkhealth.js",
    "close-all": "node ./scripts/close-all.js",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "astro": "^3.3.1",
    "rehype-katex": "^7.0.0",
